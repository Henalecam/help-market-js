{
  "name" : "Help Market",
  "version" : "1.0",
  "description" : "I'ts a extension to help you make online purchases with a compare on the best prices.",
  "manifest_version" : 3,
  "background" : {
    "service_worker": "background.js"
  },
  "action" : {
    "default_popup" : "popup.html",
    "default_icon" : {
      "16" : "images/icon16.png",
      "32" : "images/icon32.png",
      "48" : "images/icon48.png",
      "128" : "images/icon128.png"
    },
    "default_title" : "Help Market"
  },
  "permissions" : [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ]
}